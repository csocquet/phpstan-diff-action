{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "lib": [
      "es6"
    ],
    "rootDir": "./src",
    "outDir": "./lib",
    "declaration": false,
    "strict": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "exclude": [
    "node_modules",
    "__test__",
    "lib",
    "dist"
  ]
}